			<DialogLauncher Text="Spusti nastavení" 
							ShowText="True"
							Image="rp_img_btn_manual_16"
							ShowImage="True"
							CommandHandler="RP_RUN_ROADPAC.SETTINGS" />
			<!-- RibbonItem[] -->
			<RibbonButton Text="Spusti RoadPAC"
