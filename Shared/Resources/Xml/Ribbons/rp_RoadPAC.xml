<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<RibbonTab xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		   IsVisible="True" KeyTip="RP" 
		   Name="RoadPAC2" Title="RoadPAC2" IsEnabled="True" IsPanelEnabled="True" AllowTearOffContextualPanels="False">
	<!-- Panel: RoadPAC -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" IsEnabled="True">
		<RibbonPanelSource Title="RoadPAC">
			<!-- Sole purpose of this dialog launcher is to let user more options from where he can load project -->
			<DialogLauncher Text="Otevři složku projektu" CommandHandler="RP_PROSPECTOR_NEW" />
			<RibbonButton Text="Spusť&#13;RoadPAC" Name="Spusť&#13;RoadPAC" ShowText="True" Size="Large" Orientation="Vertical" ShowImage="True"
						  LargeImage="rp_logo_blue_32"
						  CommandHandler="XX:RoadpacNET7.exe" />
			<!-- Projector -->
			<RibbonButton Text="Projektor" Size="Large" Orientation="Vertical" ShowText="True" 
						  LargeImage="rp_AbstractClass_32" CommandHandler="RP_PROSPECTOR" />
			<RibbonSeparator Id="AcRibbonSeparator" SeparatorStyle="Spacer" />
			<RibbonRowPanel>
				<RibbonButton Text="Otevři (.rpsol)" ShowText="True" Image="rp_FolderBrowserDialogControl_16" CommandHandler="RP_PROSPECTOR_SOLUTION" />
				<RibbonRowBreak />
				<RibbonButton Text="Otevři projekt" ShowText="True" Image="rp_SearchFolderOpened_16" CommandHandler="RP_PROSPECTOR_NEW" />
				<RibbonRowBreak />
				<RibbonButton Text="Nový projekt" ShowText="True" Image="rp_AddEntity_16" CommandHandler="RP_PROSPECTOR_NEW" />
			</RibbonRowPanel>
		</RibbonPanelSource>
	</RibbonPanel>
  <!-- Panel: RoadCAD -->
  <RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" IsEnabled="True">
    <RibbonPanelSource Title="RoadCAD">
      <RibbonButton Text="Nastavení&#13;menu" Name="RP_CADmenu" ShowText="True" Size="Large" Orientation="Vertical" LargeImage="rp_CAD13_16" CommandHandler="MEN_CHANGE" />
    </RibbonPanelSource>
  </RibbonPanel>
	<!-- Panel: DTM -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" HighlightPanelTitleBar="False" IsEnabled="True">
		<RibbonPanelSource>
			<Title><![CDATA[Terén]]></Title>
      
			<DialogLauncher Orientation="Horizontal" IsCheckable="False" ShowImage="True" IsEnabled="True" Size="Standard" GroupLocation="Middle"
							AllowInToolBar="True" AllowInStatusBar="True" ResizeStyle="HideText"
							Text="Otevři složku s terénními modely" ShowText="False" CommandHandler="RP_COMMAND(OPEN_ACTIVE_DTM_DIRECTORY)" />
			<RibbonSplitButton Text="Terénní&#13;model" Name="RP_RUN_DTM" ShowText="True" Size="Large" Orientation="Vertical" ShowImage="True"
							   Image="rp_img_default_16" LargeImage="rp_img_default_32" CommandHandler="RP_RUN(DTM)" SynchronizeOption="Image">
				<RibbonToggleButton Text="Spusť DTM" ShowText="True" Image="rp_DTM_32" LargeImage="rp_DTM_32" />
				<RibbonToggleButton Text="Sejmi podélný řez" ShowText="True" Image="rp_LONGter_32" LargeImage="rp_LONGter_32" />
				<RibbonToggleButton Text="Sejmi příčný řez" ShowText="True" Image="rp_CROSSEXIST_16" LargeImage="rp_CROSSEXIST_32" />
				<RibbonToggleButton Text="Ornice" ShowText="True" Image="rp_Ornice_32" LargeImage="rp_Ornice_32" />
				<RibbonToggleButton Text="Geologie" ShowText="True" Image="rp_Geologie_32" LargeImage="rp_Geologie_32" />
			</RibbonSplitButton>
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: Směrové vedení trasy -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" HighlightPanelTitleBar="False">
		<RibbonPanelSource>
			<Title><![CDATA[Směrové vedení]]></Title>
			<RibbonButton Text="Spusť VIAAXI" Size="Large" ShowText="True" Orientation="Vertical"  Image="rp_Align_16" LargeImage="rp_Align_16" CommandHandler="CHANGE_VIAAXIALL"/>
			<RibbonRowPanel>
        <RibbonButton Text="Protokol" Size="Small" ShowText="True" Orientation="Vertical" Image="rp_Listing_32" LargeImage="rp_Listing_32" CommandHandler="XX:Wordpad.exe {WorkingDirectory}{Route}.L13" IsVisible="False" Tag="Route" Id="TagControllVisibility" />
				<RibbonRowBreak />
				<RibbonButton Text="Kresli&#13;2D osu" ShowText="True" Orientation="Vertical" Image="rp_Align_16" CommandHandler="testtool RP_SB" IsVisible="False" Tag="Route" Id="TagControllVisibility" />
				<RibbonRowBreak />
			</RibbonRowPanel>
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: Výškové vedení trasy -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" IsEnabled="True">
		<RibbonPanelSource>
			<Title><![CDATA[Výškové vedení]]></Title>
      rp_veral2_16
      <RibbonButton Text="Upravit&#13;niveletu" Size="Large" ShowText="True" Orientation="Vertical" ShowImage="True" LargeImage="rp_veral2_16" CommandHandler="XX:NIVW32.exe" />/>
      <RibbonRowPanel>
        <RibbonButton Text="Protokol" Size="Small" ShowText="True" Orientation="Vertical" Image="rp_Listing_32" LargeImage="rp_Listing_32" CommandHandler="XX:Wordpad.exe {WorkingDirectory}{Route}.L32" IsVisible="False" Tag="Profile" Id="TagControllVisibility" />
        <RibbonRowBreak />
        <RibbonButton Text="Kresli&#13;niveletu" ShowText="True" Orientation="Vertical" Image="rp_LONG_NEW_16" CommandHandler="testtool RP_SB" IsVisible="False" Tag="Profile" Id="TagControllVisibility" />
        <RibbonRowBreak />
				<RibbonButton Text="Tags:[Profile]" Size="Large" ShowText="True" Orientation="Vertical"
						  IsVisible="False" Tag="Profile" Id="" />
        </RibbonRowPanel>
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: Koridor & přičné úspořádání -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" IsEnabled="True">
		<RibbonPanelSource>
			<Title><![CDATA[Koridor & příčné uspořádání]]></Title>
			<RibbonButton Text="Tags:[]" Size="Large" ShowText="True" Orientation="Vertical" IsVisible="False"/>
			<RibbonButton Text="Výpočet pokrytí" Size="Large" ShowText="True" Orientation="Vertical" CommandHandler="XX:RDPVFilesUIRT.exe -D43 {WorkingDirectory} {Route}" Image="rp_PAVE_16" LargeImage="rp_PAVE_16"
              IsVisible="True" Tag="Corridor" />
			<RibbonButton Text="Výpočet koridoru" Size="Large" ShowText="True" Orientation="Vertical" CommandHandler="XX:RDPVFilesUIRT.exe -D43 {WorkingDirectory} {Route}" Image="rp_corr_16" LargeImage="rp_corr_16"
						  IsVisible="True" Tag="Corridor, CrossSection" />
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: Vytyčení -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" IsEnabled="True">
