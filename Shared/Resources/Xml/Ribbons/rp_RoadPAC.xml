<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<RibbonTab xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		   IsVisible="True" KeyTip="RP" 
		   Name="RoadPAC2" Title="RoadPAC2" IsEnabled="True" IsPanelEnabled="True" AllowTearOffContextualPanels="False">
	<!-- Panel: RoadPAC -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" HighlightPanelTitleBar="True" IsEnabled="True">
		<RibbonPanelSource Title="RoadPAC">
			<!-- Sole purpose of this dialog launcher is to let user more options from where he can load project -->
			<DialogLauncher Text="Načti složku projektu" CommandHandler="RP_PROSPECTOR_NEW" />
			<RibbonButton Text="Spusti&#13;RoadPAC" Name="Spusti&#13;RoadPAC" ShowText="True" Size="Large" Orientation="Vertical" ShowImage="True"
						  Image="rp_logo" LargeImage="rp_logo_orange"
						  CommandHandler="XX:RoadpacNET7.exe" />
			<!-- Projector -->
			<RibbonSplitButton Text="Projektor" SynchronizeOption="Image" ShowText="True" Size="Large" Orientation="Vertical" ShowImage="True">
				<RibbonToggleButton Text="Prohlížeč projektu" ShowText="True" Image="rp_AbstractClass" LargeImage="rp_AbstractClass" CommandHandler="RP_PROSPECTOR" />
				<RibbonToggleButton Text="Načti (.solution)" ShowText="True" Image="rp_AddEntity" LargeImage="rp_AddEntity" CommandHandler="RP_PROSPECTOR_SOLUTION" />
				<RibbonToggleButton Text="Načti složku projektu" ShowText="True" Image="rp_FolderBrowserDialogControl" LargeImage="rp_FolderBrowserDialogControl" CommandHandler="RP_PROSPECTOR_NEW" />
			</RibbonSplitButton>
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: DTM -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" HighlightPanelTitleBar="False" IsEnabled="True">
		<RibbonPanelSource>
			<Title><![CDATA[Terén]]></Title>
			<DialogLauncher Orientation="Horizontal" IsCheckable="False" ShowImage="True" IsEnabled="True" Size="Standard" GroupLocation="Middle"
							AllowInToolBar="True" AllowInStatusBar="True" ResizeStyle="HideText"
							Text="Otevři složku s terénními modely" ShowText="False" CommandHandler="RP_COMMAND(OPEN_ACTIVE_DTM_DIRECTORY)" />
			<RibbonSplitButton Text="Terénní&#13;práce" Name="RP_RUN_DTM" ShowText="True" Size="Large" Orientation="Vertical" ShowImage="True"
							   Image="rp_img_default_16" LargeImage="rp_img_default_32" CommandHandler="RP_RUN(DTM)" SynchronizeOption="Image">
				<RibbonToggleButton Text="Spusti DTM" ShowText="True" Image="rp_DTM_32" LargeImage="rp_DTM_32" />
				<RibbonToggleButton Text="Sejmi řez: Podélný" ShowText="True" Image="rp_CaptureFrame" LargeImage="rp_CaptureFrame" />
				<RibbonToggleButton Text="Sejmi řez: Příčný" ShowText="True" Image="rp_CaptureFrame" LargeImage="rp_CaptureFrame" />
				<RibbonToggleButton Text="Ornice" ShowText="True" Image="rp_Ornice_32" LargeImage="rp_Ornice_32" />
				<RibbonToggleButton Text="Geologie" ShowText="True" Image="rp_Geologie_32" LargeImage="rp_Geologie_32" />
			</RibbonSplitButton>
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: Směrové vedení trasy -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" HighlightPanelTitleBar="False">
		<RibbonPanelSource>
			<Title><![CDATA[Směrové vedení]]></Title>
			<RibbonButton Text="Tags:[]" Size="Large" />
			<RibbonButton Text="Tags:[]" Size="Large" 
						  IsVisible="False" Tags="Route" Id="TagControllVisibility" />
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: Výškové vedení trasy -->
	<RibbonPanel Id="RP_RBN_0005" FloatingOrientation="Vertical" CanToggleOrientation="True" HighlightPanelTitleBar="False"
			 IsEnabled="True" IsContextualTabThemeIgnored="False"
			 UID="ID_WSR_0005_07CS5">
		<RibbonPanelSource Id="RP_RBN_0005_07CS5">
			<Title><![CDATA[Výškové vedení]]></Title>
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: Koridor & přičné úspořádání -->
	<RibbonPanel Id="RP_RBN_0006" FloatingOrientation="Vertical" CanToggleOrientation="True" HighlightPanelTitleBar="False"
			 IsEnabled="True" IsContextualTabThemeIgnored="False"
			 UID="ID_WSR_0006_6614C">
		<RibbonPanelSource Id="RP_RBN_0006_6614C">
			<Title><![CDATA[Koridor & příčné uspořádání]]></Title>
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: Vytyčení -->
	<RibbonPanel Id="RP_RBN_0007" FloatingOrientation="Vertical" CanToggleOrientation="True" HighlightPanelTitleBar="False"
			 IsEnabled="True" IsContextualTabThemeIgnored="False"
			 UID="ID_WSR_0007_67FFV">
		<RibbonPanelSource Id="RP_RBN_0007_67FFV">
			<Title><![CDATA[Vytyčení]]></Title>
			<RibbonSplitButton AllowInStatusBar="True" AllowInToolBar="True" AllowOrientation="True" SynchronizeOption="Image"
							Orientation="Vertical" ResizeStyle="HideText" ShowImage="True" ShowText="True" 
							Size="Large" Image="rp_img_default_16" LargeImage="rp_img_default_32" UID="AcRibbonSplitButton">
				<Text><![CDATA[Vytyčení]]></Text>
				<RibbonToggleButton Id="RibbonToggleButton_KeepSplitButtonName" Text="Vozovky" ShowText="True" Image="rp_img_default_16" LargeImage="rp_img_default_32" />
				<RibbonToggleButton Id="RibbonToggleButton_KeepSplitButtonName" Text="Koridoru" ShowText="True" Image="rp_img_default_16" LargeImage="rp_img_default_32" />
				<RibbonToggleButton Id="RibbonToggleButton_KeepSplitButtonName" Text="Vozovky a koridoru" ShowText="True" Image="rp_img_default_16" LargeImage="rp_img_default_32" />
				<RibbonToggleButton Id="RibbonToggleButton_KeepSplitButtonName" Text="Prostorové osy" ShowText="True" Image="rp_img_default_16" LargeImage="rp_img_default_32" />
				<RibbonToggleButton Id="RibbonToggleButton_KeepSplitButtonName" Text="Trvalého záboru" ShowText="True" Image="rp_img_default_16" LargeImage="rp_img_default_32" />
			</RibbonSplitButton>
			<RibbonSeparator Id="AcRibbonSeparator" SeparatorStyle="Spacer" UID="AcRibbonSeparator" />
			<RibbonRowPanel IsTopJustified="True">
				<RibbonButton Text="Body vytyčení" Image="rp_img_default_16" />
				<RibbonRowBreak />
				<RibbonButton Text="Body vytyčení z Excelu" Image="rp_excel_16" />
				<RibbonRowBreak />
				<RibbonButton Text="Tabulka vytyčení" Image="rp_img_default_16" />
			</RibbonRowPanel>
		</RibbonPanelSource>
	</RibbonPanel>
	<!-- Panel: Nastavení -->
	<RibbonPanel Id="RP_RBN_0008" FloatingOrientation="Vertical" CanToggleOrientation="True" HighlightPanelTitleBar="False"
			 IsEnabled="True" IsContextualTabThemeIgnored="False"
			 UID="ID_WSR_0008_11IIH">
		<RibbonPanelSource Id="RP_RBN_0008_11IIH">
			<Title><![CDATA[Nastavení]]></Title>
		</RibbonPanelSource>
	</RibbonPanel>
</RibbonTab>