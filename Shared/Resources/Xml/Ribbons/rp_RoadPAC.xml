	<!-- Panel: IFC -->
	<RibbonPanel FloatingOrientation="Vertical" CanToggleOrientation="True" IsEnabled="True">
		<RibbonPanelSource>
			<Title><![CDATA[IFC]]></Title>
			<RibbonButton Text="Tags:[]" Size="Large" ShowText="True" Orientation="Vertical" />
			<RibbonButton Text="Tags:[Corridor]" Size="Large" ShowText="True" Orientation="Vertical"
						  IsVisible="False" Tag="Corridor" Id="TagControllVisibility" />
			<RibbonButton Text="Tags:[IFC]" Size="Large" ShowText="True" Orientation="Vertical"
						  IsVisible="False" Tag="IFC" Id="TagControllVisibility" />
