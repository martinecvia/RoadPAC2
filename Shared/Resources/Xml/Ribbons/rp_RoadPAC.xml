	<!-- RibbonPanel[] -->
	<RibbonPanel Id="RP_TAB_RoadPAC.Panels(0).ProjectManager"
				 IsEnabled="True"
				 HighlightPanelTitleBar="True"
				 IsVisible="True">
		<!-- RibbonPanelSource -->
		<RibbonPanelSource Id="RP_TAB_RoadPAC.Panels.ProjectManager.Id"
